CREATE OR REPLACE MODEL `proj.monitoring.memory_sarima`
OPTIONS (
  model_type = 'ARIMA_PLUS',
  time_series_timestamp_col = 'ts',
  time_series_data_col = 'value',
  time_series_id_col = 'instance_id',
  auto_arima = TRUE,
  data_frequency = 'AUTO_FREQUENCY'
) AS
SELECT
  ts,
  value,
  instance_id
FROM `proj.monitoring.cloudsql_memory`;